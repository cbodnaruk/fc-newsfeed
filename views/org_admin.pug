doctype html
html
  head
    title Megagame Space: Live Megagame Dashboards
    link(rel='stylesheet', href='/css/styles.css')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js')
    link(href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@32,400,0,0", rel="stylesheet")
    script(src='/scripts/org_admin.js')
  body 
    div#landing_info
      p welcome !{org_data.name}
      p your dashboards:
      each d in dashes 
        p
          span.dash_listing=d.dash_id
          button(class="material-symbols-outlined adminbtn") edit 
          button.adminbtn(class="material-symbols-outlined") delete
      p 
        button.adminbtn(class="material-symbols-outlined") library_add
      p  
        button.landingbtn(onClick="logout()") change password 
      p
        button.landingbtn(onClick="logout()") log out  