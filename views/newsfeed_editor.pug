- const numposts = posts.length 
- var i = numposts - 1
while i >= 0
  if posts[i].active
    div.post(id=posts[i].id+"p")
      p.text= posts[i].posttext
      p.time= 'Posted at ' + posts[i].timecode
      p
        span.edit(id=posts[i].id, onclick="beginEdit(this.id)") Edit Post
        span.delete(id=posts[i].id, onclick="deletePost(this.id)") Delete Post
  - i--